@model Setting
@{
    ViewData["Title"] = "Setting Delete";
}

<div style="color: dimgray; margin-left:15px">
    <h2>Key: @Model.Key</h2>
    <h3>Vaule: @Model.Value</h3>
</div>

<div style="display: flex; gap: 15px">
    <a class="btn btn-info" asp-action="Index">Go Back</a>
    <form method="post">
        <button type="submit" class="btn btn-danger">Delete</button>
    </form>
</div>

